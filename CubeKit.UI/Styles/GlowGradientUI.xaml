                        <icons:FluentSymbolIcon
                            Margin="0,0,2,2"
                            VerticalAlignment="Center"
                            Symbol="Dismiss20">
                            <icons:FluentSymbolIcon.Foreground>
                                <SolidColorBrush x:Name="CloseButtonIconBrush" Color="{ThemeResource HvidSortColor}" />
                            </icons:FluentSymbolIcon.Foreground>
                        </icons:FluentSymbolIcon>
                        <ToolkitControls:DropShadowPanel
                            x:Name="Glow"
                            Grid.Row="0"
